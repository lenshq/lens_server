.col-md-12
  %h1 Edit application

  .col-sm-6
    = simple_form_for @application, html: { class: 'form-horizontal' } do |f|
      = f.input :title
      = f.input :description
      = f.input :domain
      = f.input :token, disabled: true
      -#= f.association :colleagues, label_method: :id, value_method: :id
      = f.button :submit
